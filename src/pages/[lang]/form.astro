---
export const prerender = true;
export async function getStaticPaths() {
  return [
    { params: { lang: 'uz' } },
    { params: { lang: 'en' } },
    { params: { lang: 'ru' } },
  ];
}

import Layout from "../../layouts/Layout.astro";
import FormState from "../../components/FormState.jsx";
import { getLang, getStrings } from "../../utils/i18n.js";

const lang = Astro.params.lang;
const strings = await getStrings(lang);
---

<Layout lang={lang} strings={strings} title="Yoshlar Venture">
  <FormState client:load />
</Layout> 